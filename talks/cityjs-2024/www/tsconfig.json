{
  "compilerOptions": {
    "strict": true,
    "target": "esnext",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true,
    "lib": ["DOM", "DOM.Iterable", "DOM.AsyncIterable"],
    "types": ["react/experimental", "node"],
    "jsx": "react-jsx"
  }
}
